		<result property="PRODUCT_IMAGE" column="PRODUCT_IMAGE" />	
		<result property="PRODUCT_STATUS" column="PRODUCT_STATUS" />
		<result property="PRODUCT_STOCK" column="PRODUCT_STOCK" />
	</resultMap>
	
	<select id="CountBoard" parameterType="String" resultType="int">
		select count(*) from pics where trunc(PICS_DATE) = to_date(#{selectDate}) order by pics_date asc
	</select>
	
	<select id="PopularClass" resultMap="classResultMap">
		select class_name, workshop_name, class_count_member from academy order by class_count_member desc
	</select>
	
	<select id="insertMemberCount" parameterType="String" resultType="int">
		select count(*) from nagagu_member where trunc(member_date) = to_date(#{selectDate}) order by member_date asc
	</select>
	
	<select id="insertWSMemberCount" parameterType="String" resultType="int">
		select count(*) from nagagu_workshop_member where trunc(workshop_date) = to_date(#{selectDate}) order by workshop_date asc
	</select>
	
	<select id="deleteMemberCount" parameterType="String" resultType="int">
		select count(*) from delete_member where trunc(delete_sysdate) = to_date(#{selectDate})
	</select>
	
	<select id="deleteWSMemberCount" parameterType="String" resultType="int">
		select count(*) from delete_workshop_member where trunc(delete_sysdate) = to_date(#{selectDate})
	</select>
	
	<select id="myClassList" parameterType="int" resultMap="classResultMap">
		select class_name, class_count_member from academy where WORKSHOP_NUM=#{WORKSHOP_NUM} order by class_count_member desc
	</select>
	
	<select id="productSellList" parameterType="int" resultMap="productResultMap">
		select product_category, product_title, PRODUCT_SALES from PRODUCT where product_workshop=#{WORKSHOP_NUM} order by PRODUCT_SALES desc
	</select>
	
</mapper>